  contains
  attributes(global) subroutine vecadd(a, b, c, n)
    implicit none
    integer, value :: n
    real, dimension(*) :: a, b, c
    integer :: i

    i =3D threadIdx%x + (blockIdx%x - 1) * blockDim%x
    if (i &lt;=3D n) c(i) =3D a(i) + b(i)
  end subroutine vecadd
end module vecmod

program main
  use vecmod
  implicit none
  integer, parameter :: N =3D 1000000
  real, device, allocatable :: d_a(:), d_b(:), d_c(:)
  real, allocatable :: h_a(:), h_b(:), h_c(:)
  integer :: i

  allocate(h_a(N), h_b(N), h_c(N))
  h_a =3D 1.0
  h_b =3D 2.0

  allocate(d_a(N), d_b(N), d_c(N))
  d_a =3D h_a
  d_b =3D h_b

  call vecadd&lt;&lt;&lt;(N+255)/256, 256&gt;&gt;&gt;(d_a, d_b, d_c, N)

  h_c =3D d_c

  print *, "c(1) =3D", h_c(1)

  deallocate(h_a, h_b, h_c, d_a, d_b, d_c)
end program main
