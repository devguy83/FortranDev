guage-fortran"><span>program omp_vecadd
  implicit none
  integer, parameter :: N =3D 10000000
  real, allocatable :: a(:), b(:), c(:)
  integer :: i

  allocate(a(N), b(N), c(N))
  a =3D 1.0
  b =3D 2.0

  !$omp target teams distribute parallel do map(to: a, b) map(from: c)
  do i =3D 1, N
